<div class="livrosDis">
    <h1>Livros disponíveis</h1>
</div>
<div class="books-container">
    <a class="btnADD" href="/">Adicionar Livro</a>
{{#each books}}
    <div class="livros"> 
        # - {{this.id}} 
        - 
        <span class="bold">
        <a href="/books/{{this.id}}">{{this.title}}</a> 
        </span> 
        -
